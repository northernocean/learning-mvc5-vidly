@model Vidly.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>@Model.Name</p>
<ul>
    <li>Genre: @Model.GenreDetails.Name</li>
    <li>Release Date: @Model.ReleaseDate.ToString("D")</li>
    <li>Date Added: @Model.DateAdded.ToString("D")</li>
    <li>Number In Stock: @Model.NumberInStock</li>
</ul>
